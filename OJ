#include<stdio.h>
#include<string.h>
#include<stdlib.h>
//typedef struct Node;
//typedef Node *tree;
typedef struct Node {
	char data;
	tree lchild, rchild;
}Node,*tree;						
tree ptr;
int i = -1;
char s[1000];
void build(tree &ptr) {  // 建树
	if (s[++i] != '.') {
		ptr = (tree)malloc(sizeof(Node));
		ptr->data = s[i];
		build(ptr->lchild);
		build(ptr->rchild);
	}
}
void inorder(tree ptr) {  // 输出中序序列
	if (ptr) {
		inorder(ptr->lchild);
		printf("%c", ptr->data);
		inorder(ptr->rchild);
	}
}
int main() {
	while (scanf_s("%c", &s[0]) != EOF) {
		for (int j = 1; j < s[j] != '\0'; j++) {
			scanf_s("%c", &s[j]);
		}
		build(ptr);
		inorder(ptr);
	}
	return 0;
}


