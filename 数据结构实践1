#include<stdio.h>
#include<time.h>
#include<stdlib.h>
#include<time.h>
#define NAX_SIZE 2000
clock_t start, stop;
double duration;
void selsctSort(int a[], int size);
int main() {
	int i,j,t=0;
	int data[NAX_SIZE];
	srand(time(NULL));
	for (j = 0; j < 800; j++) {
		for (i = 0; i < NAX_SIZE; i++) {
			data[i] = rand();
		}
		start = clock();
		selsctSort(data, NAX_SIZE);
		stop = clock();
		duration = ((double)(stop - start)) / CLK_TCK;
		t = t + duration;
	}
	printf("%f\n", t/800);
	/*for (i = 0; i < NAX_SIZE; i++) {
		printf("%10d", data[i]);
	}
	printf("\n");*/
}
void selsctSort(int a[], int size) {
	int i, j, k,cs=0,ss=0;
	for (i = 1; i < size; i++) {
		k = i;
		for (j = i; j < size; j++) {
			++cs;
			if (k != i) {
				++ss;
				a[0] = a[k]; a[k] = a[i]; a[i] = a[0];
			}
		}
	}
}
